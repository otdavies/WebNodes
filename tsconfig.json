{
  "compilerOptions": {
    "target": "es2017",
    "module": "es6",
    "strict": true,
    "esModuleInterop": true,
    "lib": [
      "es2017",
      "dom"
    ],
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "noImplicitReturns": true,
    "outDir": "./dist/",
    "moduleResolution": "node",
  }
}